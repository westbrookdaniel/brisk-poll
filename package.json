{
  "name": "brisk-poll",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup node",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "tsc --project tsconfig.server.json",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:watch": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix watch",
    "dev:start": "cross-env NODE_ENV=development tsc-watch --project tsconfig.server.json --onSuccess \"npm run start:mocks\"",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -i ./app/styles/styles.css -o ./app/styles/tailwind.css --minify",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "cross-env NODE_ENV=production node build/server",
    "start:mocks": "node --require ./mocks build/server",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "db:reset": "prisma migrate reset",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^3.12.0",
    "@remix-run/node": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "@remix-run/serve": "^1.4.0",
    "@remix-run/server-runtime": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-utils": "^3.0.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "tiny-invariant": "^1.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@remix-run/dev": "^1.4.0",
    "@remix-run/eslint-config": "^1.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.24",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^1.3.1",
    "autoprefixer": "^10.4.4",
    "binode": "^1.0.5",
    "c8": "^7.11.0",
    "cypress": "^9.5.4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^2.55.0",
    "msw": "^0.39.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.12.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.8.2"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
